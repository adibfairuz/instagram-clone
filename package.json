{
  "name": "instagram-clone",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "lib"
    ]
  },
  "scripts": {
    "start": "nx run-many --parallel --target=start --all --maxParallel=100",
    "build": "nx run-many --parallel --target=build --all --maxParallel=100",
    "start:prod": "nx run-many --parallel --target=start:prod --all --maxParallel=100"
  },
  "devDependencies": {
    "@nx/js": "18.0.7",
    "nx": "18.0.7"
  }
}
